{
  "name": "optipng-bin",
  "version": "0.3.1",
  "description": "OptiPNG wrapper that makes it seamlessly available as a local dependency on OS X, Linux, FreeBSD and Windows",
  "keywords": [
    "png",
    "compress",
    "minify",
    "optimize",
    "img",
    "image"
  ],
  "author": {
    "name": "Sindre Sorhus",
    "email": "sindresorhus@gmail.com",
    "url": "http://sindresorhus.com"
  },
  "maintainers": [
    {
      "name": "Kevin Martensson",
      "email": "kevinmartensson@gmail.com",
      "url": "http://kevinmartensson.com"
    }
  ],
  "main": "index.js",
  "bin": {
    "optipng": "cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/yeoman/node-optipng-bin.git"
  },
  "scripts": {
    "test": "mocha --reporter list --timeout 0",
    "postinstall": "node index.js"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "dependencies": {
    "bin-wrapper": "https://github.com/kevva/bin-wrapper/archive/master.tar.gz",
    "chalk": "~0.4.0"
  },
  "devDependencies": {
    "bin-check": "~0.1.0",
    "mocha": "~1.18.2",
    "rimraf": "~2.2.6"
  },
  "files": [
    "cli.js",
    "index.js"
  ]
